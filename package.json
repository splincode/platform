{
  "name": "ng-easy",
  "version": "0.0.0",
  "description": "Monorepo for Angular tooling and libraries",
  "keywords": [
    "angular",
    "nx"
  ],
  "author": "samuelfernandez",
  "homepage": "https://github.com/ng-easy/platform#readme",
  "bugs": {
    "url": "https://github.com/ng-easy/platform/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-easy/platform.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2015 browser module main",
    "release": "nx run-many --target=release --all",
    "ng": "nx",
    "nx": "nx",
    "start": "nx serve --hmr",
    "build": "nx run-many --target=build --all --configuration production --parallel",
    "build:serve": "http-server dist/apps/core-web-vitals-app/browser",
    "test": "nx run-many --target=test --all --parallel",
    "lint:workspace": "nx workspace-lint",
    "lint": "nx run-many --target=lint --all --parallel",
    "e2e": "nx run-many --target=e2e --all --configuration production",
    "e2e:update-snapshots": "cross-env updateSnapshots=true npm run e2e",
    "favicons": "nx run core-web-vitals-app:favicons:production",
    "affected:dep-graph": "nx affected:dep-graph",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lhci": "lhci autorun"
  },
